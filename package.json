{
  "name": "underkill-stack",
  "version": "1.0.0",
  "description": "A minimal react-router stack",
  "main": "index.js",
  "type": "module",
  "packageManager": "bun@1.2.2",
  "imports": {
    "#*": "./*"
  },
  "scripts": {
    "dev": "react-router dev",
    "cli": "bun run cli.ts",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:studio": "bunx drizzle-kit studio",
    "db:generate": "bunx drizzle-kit generate",
    "db:push": "bunx drizzle-kit push",
    "db:pull": "bunx drizzle-kit pull",
    "db:migrate:local": "bunx drizzle-kit migrate --schema=./db/schema.ts --out=./db/migrations",
    "build": "react-router build",
    "start": "NODE_ENV=production react-router-serve ./build/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobparis/underkill-stack.git"
  },
  "keywords": [
    "remix"
  ],
  "author": "Jacob Paris",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jacobparis/underkill-stack/issues"
  },
  "homepage": "https://github.com/jacobparis/underkill-stack#readme",
  "dependencies": {
    "@react-router/node": "^7.1.3",
    "@react-router/serve": "^7.1.3",
    "@tailwindcss/vite": "^4.0.0",
    "@types/better-sqlite3": "^7.6.12",
    "better-sqlite3": "^11.8.1",
    "bun": "^1.2.2",
    "drizzle-kit": "^0.30.4",
    "drizzle-orm": "^0.39.2",
    "isbot": "^5.1.21",
    "nuqs": "^2.3.2",
    "ora": "^8.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.3",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@react-router/dev": "^7.1.3",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/bun": "^1.2.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11"
  }
}